#!/usr/local/bin/omi
/* 
 * Casos booleanos para operaciones aritméticas:
 */
<< "-- Suma";
<< true + false;
<< false + true;
<< true + true;
<< false + false;
<< "-- Resta";
<< true - false;
<< false - true;
<< true - true;
<< false - false;
<< "-- Producto";
<< true * false;
<< false * true;
<< true * true;
<< false * false;
<< "-- División";
<< "ERROR[";
<< true / false;
<< "]";
<< false / true;
<< true / true;
<< "ERROR[";
<< false / false;
<< "]";
<< "-- Módulo";
<< "ERROR[";
<< true % false;
<< "]";
<< false % true;
<< true % true;
<< "ERROR[";
<< false % false;
<< "]";
<< "Potencia";
<< true ^ false;
<< false ^ true;
<< true ^ true;
<< false ^ false;
 
<< "Preincremento";
<< "ERROR[";
<< ++true;
<< ++false;
<< "]";

<< "Postincremento";
<< "ERROR[";
<< true++;
<< false++;
<< "]";

<< "Predecremento";
<< "ERROR[";
<< --true;
<< --false;
<< "]";

<< "Postdecremento";
<< "ERROR[";
<< true--;
<< false--;
<< "]";

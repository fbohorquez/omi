#!/usr/local/bin/omi
/* 
 * Casos para operaciones boleanas:
 */
<< "-- Igualdad";
<< (true == false);
<< (false == true);
<< (true == true);
<< (false == false);
<< "-- Desigualdad";
<< (true != false);
<< (false != true);
<< (true != true);
<< (false != false);
<< "-- Negación"; 
<< (!true);
<< (!false);
<< "-- Mayor";
<< true > false;
<< false > true;
<< true > true;
<< false > false;
<< "-- Menor";
<< true < false;
<< false < true;
<< true < true;
<< false < false;
<< "-- Mayor o igual";
<< true >= false;
<< false >= true;
<< true >= true;
<< false >= false;
<< "-- Menor o igual";
<< true <= false;
<< false <= true;
<< true <= true;
<< false <= false;
<< "-- Idéntico";
<< true === false;
<< false === true;
<< true === true;
<< false === false;
<< "-- No idéntico";
<< true !== false;
<< false !== true;
<< true !== true;
<< false !== false;
<< "-- Is null";
<< isnull (true);
<< isnull (false);
<< "-- Is empty";
<< empty (true);
<< empty (false);
<< "-- AND";
<< true && false;
<< false && true;
<< true && true;
<< false && false;
<< "-- OR";
<< true || false;
<< false || true;
<< true || true;
<< false || false;
